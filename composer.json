{
    "name": "sunaoka/google-play-scraper",
    "description": "A PHP scraper to get app data from Google Play",
    "keywords": ["google play", "scraper"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org",
            "role": "Developer"
        },
        {
            "name": "Muhammad Tayyab Sheikh",
            "email": "cstayyab@gmail.com",
            "homepage": "https://cstayyab.com",
            "role": "Ex-Maintainer"
        },
        {
            "name": "Raul Rodriguez",
            "email": "raul@raulr.net",
            "role": "Ex-Maintainer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-dom": "*",
        "fabpot/goutte": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\GooglePlayScraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\GooglePlayScraper\\Tests\\": "tests/"
        }
    }
}
